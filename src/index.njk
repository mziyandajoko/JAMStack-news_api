---
title: news feed Api
layout: "base.njk"
---
<h1>{{title}}</h1>
<div class="container">
    {% for article in news_api.articles %}
      <div class="content-wrapper">
          <a href="{{article.url}}" target="_blank">
          <div><img src="{{article.urlToImage}}" width="100%" height="300"> </div>
          <h4>
              {{ article.title}}
            </a>
          </h4>
          <span>{{ article.publishedAt}} | {{article.author }}</span>
          <div>{{ article.description }}</div>
      </div>
    {% endfor %}
    
</div>